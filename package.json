  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --clear-screen=false server/index.ts",
    "build": "vite build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node dist/server/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:property": "vitest run --grep 'Property'",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:seed": "tsx server/seed.ts",
    "db:reset": "drizzle-kit push && npm run db:seed",
    "create:superadmin": "tsx server/createSuperAdmin.ts"
  },
